model User {
    id                     String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
    name                   String
    email                  String    @unique
    password               String
    nickname               String?   @unique
    redefinition_password  Boolean?  @default(false)
    created_at             DateTime  @default(now())
    updated_at             DateTime?
}